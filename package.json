{
	"name": "txt",
	"displayName": "txt",
    "description": "Test Extension",
    "publisher": "test",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/AlexTugarev/txt"
    },
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
            {
                "command": "extension.helloWorld",
                "category": "TXT",
                "title": "Hello World"
            },
            {
                "command": "extension.info",
                "category": "TXT",
                "title": "Info"
            },
            {
                "command": "extension.warning",
                "category": "TXT",
                "title": "Warning"
            },
            {
                "command": "extension.error",
                "category": "TXT",
                "title": "Error"
            },
            {
                "command": "extension.progress",
                "category": "TXT",
                "title": "Progress"
            },
            {
                "command": "extension.withProgress",
                "category": "TXT",
                "title": "withProgress"
            }
        ]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.35.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.0-next.0"
	}
}
